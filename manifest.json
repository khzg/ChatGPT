{
  "icons": {
    "32": "icon-32.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "Ai Chat everywhere",
  "version": "17.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "sidebar_action": {
    "default_title": "Ai Chat everywhere",
    "default_panel": "popup.html",
    "default_icon": "icon.png",
    "browser_style": false
  },
  "permissions": ["history", "storage"],
  "background": {
    "scripts": ["background.js"]
  }
}
